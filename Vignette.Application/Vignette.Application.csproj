<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Configurations>Debug;Release</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningsAsErrors></WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FaceRecognitionDotNet.CUDA92" Version="1.3.0.2" />
    <PackageReference Include="ppy.osu.Framework" Version="2021.128.0" />
    <PackageReference Include="Vignette.Application.Camera" Version="2021.203.0" />
    <PackageReference Include="Vignette.Application.Live2D" Version="2021.201.4" />
    <PackageReference Include="Vignette.Application.Live2D.Native" Version="2021.201.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Recognition\Models\*.dat">
      <Link>recognition/models/%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
